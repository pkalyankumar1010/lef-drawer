{
  "name": "lef-viz",
  "displayName": "LEF Visualizer",
  "description": "Draw LEF (Library Exchange Format) with interactive web-based previews.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.101.0"
  },
  "publisher": "pkalyankumar1010",
  "icon": "lef_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/pkalyankumar1010/lef-drawer.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lef-viz.helloWorld",
        "title": "LEF : Yello World"
      },
      {
        "command": "lef-viz.showPreview",
        "title": "LEF : Show Preview"
      }
    ],
    "keybindings": [
  {
    "command": "lef-viz.showPreview",
    "key": "ctrl+shift+v",
    "when": "editorLangId == lef"
  }
    ],
    "languages": [
      {
        "id": "lef",
        "extensions": [".lef"],
        "aliases": ["LEF", "lef"]
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/assert": "^1.5.11",
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.101.0",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-web": "^0.0.66",
    "assert": "^2.1.0",
    "eslint": "^9.19.0",
    "mocha": "^11.1.0",
    "process": "^0.11.10",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/node": "^24.0.8",
    "path-browserify": "^1.0.1"
  }

}
